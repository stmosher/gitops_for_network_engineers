service password-encryption
!
hostname ip-10-0-0-10
!
no aaa new-model
!
spanning-tree extend system-id
!
interface Loopback0
  ip address 172.16.0.10 255.255.255.255
!
interface Tunnel1
  ip address 172.16.1.1 255.255.255.0
  tunnel source 10.0.0.10
  tunnel destination 10.0.0.11
  no shutdown
!
interface GigabitEthernet1
 ip address dhcp
 negotiation auto
 no mop enabled
 no mop sysid
!
ip ssh rsa keypair-name ssh-key
ip ssh version 2
!
line con 0
 stopbits 1
line vty 0 4
 exec-timeout 0 0
 login local
 transport input ssh
line vty 5 20
 login local
 transport input ssh
